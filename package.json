{
  "name": "nuxt-2-template",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": {
    "name": "YES Studio",
    "email": "admin@yesstudio.co.uk",
    "url": "https://yes.studio"
  },
  "scripts": {
    "build": "nuxt build",
    "commit": "cz",
    "dev": "nuxt dev",
    "format": "npm run format:eslint && npm run format:stylelint && npm run format:prettier",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "prettier --ignore-path .gitignore --list-different --write .",
    "format:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint:stylelint": "stylelint \"**/*.{scss,vue}\" --ignore-path .gitignore",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "start": "nuxt start"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@yesstudio/yes-sanitise": "^2.1.5",
    "@yesstudio/yes-utils": "^1.13.3",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.25.5",
    "http-status-codes": "^2.2.0",
    "nuxt": "^2.15.8",
    "vue": "^2.7.11",
    "vue-server-renderer": "^2.7.11",
    "vue-template-compiler": "^2.7.11",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@capsizecss/unpack": "^0.1.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "postcss-sort-media-queries": "^4.3.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^10.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
